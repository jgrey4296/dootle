digraph list {
label=MainMachine;
fontname=Arial;
fontsize=10;
rankdir=LR;
i [shape=circle, style=filled, fontsize=1, fixedsize=true, width=0.2, height=0.2, fillcolor=black];
i -> init [label="", color=blue, fontname=Arial, fontsize=9];
init [label=Init, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=1, penwidth=2, fillcolor=turquoise];
init -> setup [label=progress, color=blue, fontname=Arial, fontsize=9];
setup [label=Setup, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=1, fillcolor=white];
setup -> plugins [label=progress, color=blue, fontname=Arial, fontsize=9];
setup -> failed [label=fail, color=blue, fontname=Arial, fontsize=9];
plugins [label=Plugins, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=1, fillcolor=white];
plugins -> cli [label=progress, color=blue, fontname=Arial, fontsize=9];
plugins -> failed [label=fail, color=blue, fontname=Arial, fontsize=9];
cli [label=Cli, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=1, fillcolor=white];
cli -> reporter [label=progress, color=blue, fontname=Arial, fontsize=9];
cli -> failed [label=fail, color=blue, fontname=Arial, fontsize=9];
reporter [label=Reporter, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=1, fillcolor=white];
reporter -> commands [label=progress, color=blue, fontname=Arial, fontsize=9];
reporter -> failed [label=fail, color=blue, fontname=Arial, fontsize=9];
commands [label=Commands, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=1, fillcolor=white];
commands -> tasks [label=progress, color=blue, fontname=Arial, fontsize=9];
commands -> failed [label=fail, color=blue, fontname=Arial, fontsize=9];
tasks [label=Tasks, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=1, fillcolor=white];
tasks -> failed [label="progress\n[should_fail]", color=blue, fontname=Arial, fontsize=9];
tasks -> run [label=progress, color=blue, fontname=Arial, fontsize=9];
tasks -> failed [label=fail, color=blue, fontname=Arial, fontsize=9];
run [label=Run, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=1, fillcolor=white];
run -> report [label=progress, color=blue, fontname=Arial, fontsize=9];
run -> failed [label=fail, color=blue, fontname=Arial, fontsize=9];
failed [label=Failed, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=1, fillcolor=white];
failed -> report [label=progress, color=blue, fontname=Arial, fontsize=9];
report [label=Report, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=1, fillcolor=white];
report -> shutdown [label=progress, color=blue, fontname=Arial, fontsize=9];
shutdown [label=Shutdown, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=1, fillcolor=white];
shutdown -> finished [label=progress, color=blue, fontname=Arial, fontsize=9];
finished [label=Finished, shape=rectangle, style="rounded, filled", fontname=Arial, fontsize=10, peripheries=2, fillcolor=white];
}
